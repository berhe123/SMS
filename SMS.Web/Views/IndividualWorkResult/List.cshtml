@{
    ViewBag.Title = "List of Assignment Result";
    Layout = "~/Views/Shared/_TeacherLayout.cshtml";
}
@section scripts {
    <script src="~/Scripts/Views/IndividualWorkResult/IndividualWorkResult_List4.js" type="text/javascript"></script>
}
<input id="mnu-active" type="hidden" value="mnuIndividualWorkResultList" />
<section class="dashboard-counts no-padding-bottom">
    <div class="container-fluid">
        <div class="row-fluid bg-white has-shadow">
            <!-- Item -->
            <div class="span2 text-center">
                <a href="~/IndividualWorkSchedule/New" style="text-decoration:none;" class="mnuIndividualWorkScheduleNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div style="margin-left:20px;">
                            <div class="span4 icon bg-orange"><i class="icon-form"></i></div>
                            <div class="span3 title">Assignment</div>
                        </div>
                    </div>
                </a>
            </div>
            <div class="span3 text-center">
                <a href="~/GroupName/New" style="text-decoration:none;" class="mnuGroupNameNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span4 title">Group Work</div>
                    </div>
                </a>
            </div>
            <div class="span2 text-center">
                <a href="~/QuizSchedule/New" style="text-decoration:none;" class="mnuQuizScheduleNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span3 title">Quiz</div>
                    </div>
                </a>
            </div>
            <div class="span3 text-center">
                <a href="~/TestSchedule/New" style="text-decoration:none;" class="mnuTestScheduleNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span3 title">Test</div>
                    </div>
                </a>
            </div>
            <div class="span2 text-center">
                <a href="~/ExerciseBookResult/New" style="text-decoration:none;" class="mnuExerciseBookResultNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span4 title">Exercise Book</div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</section>
<br />
<div class="client card">
    <div class="row-fluid bg-light has-shadow">
        <div class="span12">
            <div class="card">
                <ul class="nav nav-tabs">
                    <li><a href="~/IndividualWorkResult/New" style="text-decoration:none;">New</a></li>
                    <li class="active"><a href="~/IndividualWorkResult/List" style="text-decoration:none;">Manage</a></li>
                    <li style="float:right;"><a href="~/Teacher/TeacherIndex" style="text-decoration:none;">Home</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="row-fluid bg-light has-shadow" style="text-align:center;">
        <h2 class="no-margin-bottom" style="display: inline; float:left; font-weight:normal;">List of Assignment Result</h2>
    </div>
    <br />
    <div class="row-fluid">
        <div class="statistic span3">
            <div class="row-fluid">
                <div class="span10">
                    <a href="~/IndividualWorkSchedule/New" style="text-decoration:none;" class="mnuIndividualWorkScheduleNew">
                        <div class="row-fluid statistic d-flex align-items-center bg-white has-shadow">
                            <div class="span6 icon bg-gray"><i class="fa fa-tasks"></i></div>
                            <div class="span6 text"><small>Assignment</small><br /><small>Schedule</small></div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span10">
                    <a href="~/IndividualWorkResult/New" style="text-decoration:none;" class="mnuIndividualWorkResultNew">
                        <div class="row-fluid statistic d-flex align-items-center bg-white has-shadow">
                            <div class="span6 icon bg-info"><i class="fa fa-tasks"></i></div>
                            <div class="span6 text"><small>Assignment</small><br /><strong>Result</strong></div>
                        </div>
                    </a>
                </div>
            </div>
        </div>
        <div class="chart span9">
            <div class="row-fluid" style="margin-left:-5px;">
                @Helpers.Select("IndividualWork", "Assignment", "IndividualWorks()", "IndividualWorkGuid", 52)
                <br />
                <button id="btnShowStudents" class="btn btn-secondary"> Show Students </button>
            </div>
            <br/>
            <div class="row-fluid" style="margin-left:-5px;">
                <table class="table table-bordered table-striped table-condensed" id="tblStudent">
                    <thead>
                        <tr>
                            <th style="width:46%"> Student Name </th>
                            <th style="width:15%">Result</th>
                            <th style="width:35%">Comment</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td colspan="5" class="dataTables_empty">
                                Loading data from server
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
              
            <div class="row-fluid">
                <div class="span8" style="margin-bottom:5px; margin-right:5px;">
                    <button id="btnUpdateResult" class="btn btn-primary">Update Result</button>
                </div>
            </div>  
                                
        </div>
    </div>
</div>