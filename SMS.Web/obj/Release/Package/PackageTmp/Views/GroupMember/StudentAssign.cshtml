@{
    ViewBag.Title = "Student Assign ";
    Layout = "~/Views/Shared/_TeacherLayout.cshtml";
}
@section scripts{
    <script src="~/Scripts/Views/GroupMember/GroupMember_StudentAssign.js" type="text/javascript"></script>
}
<input id="mnu-active" type="hidden" value="mnuStudentAssignNew" />
<input id="txtId" type="hidden" value="@Model" />
<section class="dashboard-counts no-padding-bottom">
    <div class="container-fluid">
        <div class="row-fluid bg-white has-shadow">
            <!-- Item -->
            <div class="span2 text-center">
                <a href="~/IndividualWorkSchedule/New" style="text-decoration:none;" class="mnuIndividualWorkScheduleNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div style="margin-left:20px;">
                            <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                            <div class="span3 title">Assignment</div>
                        </div>
                    </div>
                </a>
            </div>
            <div class="span3 text-center">
                <a href="~/GroupName/New" style="text-decoration:none;" class="mnuGroupNameNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-orange"><i class="icon-form"></i></div>
                        <div class="span4 title">Group Work</div>
                    </div>
                </a>
            </div>
            <div class="span2 text-center">
                <a href="~/QuizSchedule/New" style="text-decoration:none;" class="mnuQuizScheduleNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span3 title">Quiz</div>
                    </div>
                </a>
            </div>
            <div class="span3 text-center">
                <a href="~/TestSchedule/New" style="text-decoration:none;" class="mnuTestScheduleNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span3 title">Test</div>
                    </div>
                </a>
            </div>
            <div class="span2 text-center">
                <a href="~/ExerciseBookResult/New" style="text-decoration:none;" class="mnuExerciseBookResultNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span4 title">Exercise Book</div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</section>
<br />
<div class="client card">
    <div class="row-fluid bg-light has-shadow">
        <div class="span12">
            <div class="card">
                <ul class="nav nav-tabs">
                    <li><a href="~/GroupName/New" style="text-decoration:none;">New</a></li>
                    <li><a href="~/GroupName/List" style="text-decoration:none;">Manage</a></li>
                    <li class="active"><a href="~/GroupMember/StudentAssign" style="text-decoration:none;">Student Assign</a></li>
                    <li style="float:right;"><a href="~/Teacher/TeacherIndex" style="text-decoration:none;">Home</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="row-fluid bg-light has-shadow" style="text-align:center;">
        <h2 class="no-margin-bottom" style="display: inline; float:left; font-weight:normal;"> Student Assign </h2>
    </div>
    <br />
    <div class="row-fluid">
        <div class="statistic span3">
            <div class="row-fluid">
                <div class="span8">
                    <a href="~/GroupMember/New" style="text-decoration:none;" class="mnuGroupMemberNew">
                        <div class="row-fluid statistic d-flex align-items-center bg-white has-shadow">
                            <div class="span6 icon bg-info"><i class="fa fa-tasks"></i></div>
                            <div class="span6 text"><strong>Create</strong><br /><small>Group Member</small></div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span8">
                    <a href="~/GroupWorkSchedule/New" style="text-decoration:none;" class="mnuGroupWorkScheduleNew">
                        <div class="row-fluid statistic d-flex align-items-center bg-white has-shadow">
                            <div class="span6 icon bg-gray"><i class="fa fa-tasks"></i></div>
                            <div class="span6 text"><small>Group Work Schedule</small></div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span8">
                    <a href="~/GroupWorkScheduleResult/New" style="text-decoration:none;" class="mnuGroupWorkResultNew">
                        <div class="row-fluid statistic d-flex align-items-center bg-white has-shadow">
                            <div class="span6 icon bg-gray"><i class="fa fa-tasks"></i></div>
                            <div class="span6 text"><small>Group Work Result</small></div>
                        </div>
                    </a>
                </div>
            </div>            
        </div>
        <div class="chart span9">
            <div class="row-fluid">
                <div class="span6">
                    <div class="row-fluid">
                        @Helpers.Select("ClassRoom", "Class Room", "ClassRooms()", "ClassRoomGuid", 52)
                        @Helpers.Select("Subject", "Subject", "Subjects()", "SubjectTeacherClassRoomGuid", "disabled", 52)
                    </div>
                    <br/>
                    <div class="row-fluid">
                        <table class="table table-bordered table-striped table-condensed" id="tblGroupName">
                            <thead>
                                <tr>
                                    <th style="width:39%">
                                        Group Name
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td colspan="3" class="dataTables_empty">
                                        Loading data from server
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="row-fluid">
                        <div class="control-group">
                            <a id="btnSave" class="btn btn-primary">Save</a>
                            <a id="btnReset" class="btn btn-secondary">Reset</a>
                        </div>
                    </div>
                </div>
                <div class="span5">
                    <table class="table table-bordered table-striped table-condensed" id="tblStudent">
                        <thead>
                            <tr>
                                <th style="width:5%">
                                    Select
                                </th>
                                <th style="width:39%">
                                    Student Name
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td colspan="3" class="dataTables_empty">
                                    Loading data from server
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>           
            </div>
    </div>
</div>
