@{
    ViewBag.Title = "List of Group Work Schedule";
    Layout = "~/Views/Shared/_TeacherLayout.cshtml";
}
@section scripts {
    <script src="~/Scripts/Views/GroupWorkSchedule/GroupWorkSchedule_List.js" type="text/javascript"></script>
}
<input id="mnu-active" type="hidden" value="mnuGroupWorkScheduleList" />
<section class="dashboard-counts no-padding-bottom">
    <div class="container-fluid">
        <div class="row-fluid bg-white has-shadow">
            <!-- Item -->
            <div class="span2 text-center">
                <a href="~/IndividualWorkSchedule/New" style="text-decoration:none;" class="mnuIndividualWorkScheduleNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div style="margin-left:20px;">
                            <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                            <div class="span3 title">Assignment</div>
                        </div>
                    </div>
                </a>
            </div>
            <div class="span3 text-center">
                <a href="~/GroupName/New" style="text-decoration:none;" class="mnuGroupNameNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-orange"><i class="icon-form"></i></div>
                        <div class="span4 title">Group Work</div>
                    </div>
                </a>
            </div>
            <div class="span2 text-center">
                <a href="~/QuizSchedule/New" style="text-decoration:none;" class="mnuQuizScheduleNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span3 title">Quiz</div>
                    </div>
                </a>
            </div>
            <div class="span3 text-center">
                <a href="~/TestSchedule/New" style="text-decoration:none;" class="mnuTestScheduleNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span3 title">Test</div>
                    </div>
                </a>
            </div>
            <div class="span2 text-center">
                <a href="~/ExerciseBookResult/New" style="text-decoration:none;" class="mnuExerciseBookResultNew">
                    <div class="row-fluid item d-flex align-items-center">
                        <div class="span4 icon bg-gray"><i class="icon-form"></i></div>
                        <div class="span4 title">Exercise Book</div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</section>
<br />
<div class="client card">
    <div class="row-fluid bg-light has-shadow">
        <div class="span12">
            <div class="card">
                <ul class="nav nav-tabs">
                    <li><a href="~/GroupWorkSchedule/New" style="text-decoration:none;">New</a></li>
                    <li class="active"><a href="~/GroupWorkSchedule/List" style="text-decoration:none;">Manage</a></li>
                    <li style="float:right;"><a href="~/Teacher/TeacherIndex" style="text-decoration:none;">Home</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="row-fluid bg-light has-shadow" style="text-align:center;">
        <h2 class="no-margin-bottom" style="display: inline; float:left; font-weight:normal;">List of Group Work Schedule</h2>
    </div>
    <br />
    <div class="row-fluid">
        <div class="statistic span3">
            <div class="row-fluid">
                <div class="span8">
                    <a href="~/GroupName/New" style="text-decoration:none;" class="mnuGroupNameNew">
                        <div class="row-fluid statistic d-flex align-items-center bg-white has-shadow">
                            <div class="span6 icon bg-gray"><i class="fa fa-tasks"></i></div>
                            <div class="span6 text"><small>Create Group Member</small></div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span8">
                    <a href="~/GroupWorkSchedule/New" style="text-decoration:none;" class="mnuGroupWorkScheduleNew">
                        <div class="row-fluid statistic d-flex align-items-center bg-white has-shadow">
                            <div class="span6 icon bg-info"><i class="fa fa-tasks"></i></div>
                            <div class="span6 text"><small>Group Work</small><br /><strong>Schedule</strong></div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span8">
                    <a href="~/GroupWorkResult/New" style="text-decoration:none;" class="mnuGroupWorkResultNew">
                        <div class="row-fluid statistic d-flex align-items-center bg-white has-shadow">
                            <div class="span6 icon bg-gray"><i class="fa fa-tasks"></i></div>
                            <div class="span6 text"><small>Group Work Result</small></div>
                        </div>
                    </a>
                </div>
            </div>            
        </div>        
        <div class="chart span9">
            <div class="row-fluid" style="margin-left:-5px;">
                @Helpers.Select("ClassRoomSubject", "Class Room", "ClassRoomSubjects()", "SubjectTeacherClassRoomGuid", 52)
            </div>
            <br />
            <div class="row-fluid" style="margin-left:-5px;">
                <table class="table table-bordered table-striped table-condensed" id="tblGroupWorkSchedule">
                    <thead>
                        <tr>
                            <th width="3%" class="action-button">
                            </th>
                            <th width="25%">
                                Group Work
                            </th>
                            <th width="24%">
                                Title
                            </th>
                            <th width="10%">
                                Given Date
                            </th>
                            <th width="10%">
                                Submission Date
                            </th>
                            <th width="5%">
                                OutOf
                            </th>
                            <th width="20%">
                                Information
                            </th>
                            <th width="3%" class="action-button">
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td colspan="5" class="dataTables_empty">
                                Loading data from server
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>